{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "This is the auth service for mern space project",
  "scripts": {
    "build": "rimraf ./dist && swc ./src -d dist",
    "dev": "cross-env NODE_ENV=development nodemon src/server.ts",
    "dev:bun": "cross-env NODE_ENV=development bun --hot src/server.ts",
    "format": "prettier .",
    "format:check": "bun format -- --check",
    "format:fix": "bun format -- --write",
    "lint": "eslint .",
    "lint:fix": "bun lint -- --fix",
    "prepare": "husky install",
    "start": "ts-node src/index.ts",
    "start:bun": "bun build/server.js",
    "test": "cross-env NODE_ENV=test jest --watch --runInBand",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "keywords": [],
  "author": {
    "name": "Shivam Vijaywargi",
    "email": "vijaywargishivam@gmail.com",
    "url": "https://github.com/shivamvijaywargi"
  },
  "maintainers": [
    {
      "name": "Shivam Vijaywargi",
      "email": "vijaywargishivam@gmail.com",
      "url": "https://github.com/shivamvijaywargi"
    }
  ],
  "license": "ISC",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.85",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.5",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --max-warnings=0",
      "prettier --write"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.3.17",
    "winston": "^3.10.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
